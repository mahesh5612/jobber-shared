{
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "description": "Helpers library for Jobber app",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && tsc --outDir build/src && node scripts/build-package.js",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/cjs --source-maps",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/esm --source-maps"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "name": "@mahesh5612/jobber-shared",
  "author": "Mahesh Nahta",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahesh5612/jobber-shared.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
  },
  "types": "./src/index.d.ts",
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "cloudinary": "^2.5.1",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.10.1",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "cross-env": "^7.0.3"
  }
}
